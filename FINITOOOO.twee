/* ============ LAB UI ============ */
.lab-hud{
  position: sticky;
  top: 0;
  display: flex;
  gap: 12px;
  align-items: center;
  margin-bottom: 8px;
  z-index: 5;
}
.lab-timer{
  padding: 6px 10px;
  border: 2px solid #fff2;
  border-radius: 8px;
  background: #0008;
  font-weight: 700;
}
.lab-timer.warn{ color: #ffd166; border-color:#ffd16655; }
.lab-timer.danger{ color:#ff4d4d; border-color:#ff4d4d66; animation: pulseRed 0.9s infinite; }
@keyframes pulseRed{
  0%{ transform: scale(1);}
  50%{ transform: scale(1.05);}
  100%{ transform: scale(1);}
}
.lab-alert{
  padding: 6px 10px;
  border: 2px solid #fff2;
  border-radius: 8px;
  background: #0008;
}

.lab-map{
  position: fixed;
  top: 14px;
  right: 14px;
  background: #0009;
  border: 2px solid #fff2;
  padding: 8px;
  border-radius: 10px;
  z-index: 10;
}
.lab-map-row{ display: flex; }
.lab-map .cell{
  width: 16px; height:16px; margin: 2px; border-radius: 3px;
  background: #222;
  box-shadow: inset 0 0 4px #000;
}
.lab-map .cell.explored{ background:#444; }
.lab-map .cell.current{ background:#00d1ff; box-shadow: 0 0 8px #00d1ff; }
.lab-map .cell.exit{ outline: 2px solid #00ff85; }

.lab-room{
  display: grid;
  grid-template-columns: 1fr;
  gap: 12px;
}

.lab-room-image-wrap{
  position: relative;
  overflow: hidden;
  border-radius: 12px;
  border: 2px solid #ff1a1a;
}
.lab-room-image{
  width: 100%;
  height: auto;
  display: block;
  filter: contrast(1.05) saturate(1.05);
}
.lab-room-badge{
  position: absolute;
  top: 10px; left: 10px;
  background: #00ff8544;
  border: 2px solid #00ff85aa;
  color: #fff; padding: 4px 8px;
  border-radius: 8px; font-weight: 700;
  backdrop-filter: blur(2px);
}
.lab-room-badge.lit{
  background: #ff3b3b55;
  border-color: #ff3b3b99;
}

.lab-move-grid{
  display: grid;
  grid-template-columns: 1fr 1fr 1fr;
  gap: 8px;
  max-width: 420px;
}
.lab-actions .button.secondary{
  background:#222; border-color:#666; margin-top:6px;
}
.lab-actions .button:disabled{
  opacity:.5; filter: grayscale(0.6); pointer-events: none;
}

.lab-msg{
  opacity:0; transform: translateY(6px);
  transition: all .2s ease;
  font-weight:700;
}
.lab-msg.ok{ color:#00ff85; }
.lab-msg.warn{ color:#ff4d4d; }
.lab-msg.show{ opacity:1; transform: translateY(0); }

/* ============ Glitch ============ */
.glitch{
  position: relative;
  display:inline-block;
  font-weight: 900;
  text-transform: uppercase;
  letter-spacing: 1px;
  animation: glitchJitter 2s infinite;
}
.glitch::before, .glitch::after{
  content: attr(data-text);
  position:absolute; top:0; left:0; right:0;
  clip-path: inset(0 0 0 0);
}
.glitch::before{
  color:#ff1f8f; transform: translate(1px, -1px);
  mix-blend-mode: screen; opacity: .7;
}
.glitch::after{
  color:#00d1ff; transform: translate(-1px, 1px);
  mix-blend-mode: screen; opacity: .7;
}
@keyframes glitchJitter{
  0%{ transform: none;}
  20%{ transform: skew(-0.5deg); }
  40%{ transform: translateX(1px);}
  60%{ transform: skew(0.6deg);}
  80%{ transform: translateX(-1px);}
  100%{ transform: none;}
}

/* ---------- SCENA / STANZA ---------- */
.room {
  position: relative;
  background: linear-gradient(#e8d7a9 55%, #6b4aa6 55%); /* parete + pavimento viola */
  border: 4px solid #2b2342;
  border-radius: 14px;
  padding: 18px 18px 26px;
  box-shadow: 0 10px 0 #2b2342, inset 0 0 0 3px #fff3;
  overflow: hidden;
}

/* titolo insegna sopra la porta */
.room .sign {
  display: inline-block;
  padding: 6px 14px;
  background: #fff;
  color: #1a1a1a;
  border: 3px solid #2b2342;
  border-radius: 6px;
  font-weight: 900;
  letter-spacing: 1px;
  box-shadow: 0 4px 0 #2b2342;
}

/* porta cartoon */
.door {
  width: 280px;
  height: 360px;
  margin: 14px auto;
  background: #7b4a28;
  border: 8px solid #3e2413;
  border-radius: 8px;
  position: relative;
  box-shadow: inset -10px 0 0 #6a3f22, 0 8px 0 #2b2342;
}
.door::after{ /* maniglia */
  content:"";
  position:absolute; right:26px; top:55%;
  width:16px; height:16px; border-radius:50%;
  background:#d3a02f; box-shadow: inset -3px -3px 0 #956f18;
}

/* ---------- ENTRATA SCENA ---------- */
.anim-enter {
  animation: enterPop 600ms cubic-bezier(.2,1.2,.2,1) both;
}
@keyframes enterPop {
  0% { transform: translateY(24px) scale(.96); filter: saturate(.6) blur(1px); opacity: 0; }
  60%{ transform: translateY(-6px) scale(1.02); filter: none; opacity: 1; }
  100%{ transform: translateY(0) scale(1); }
}

/* ---------- SQUASH & STRETCH (respiro cartoon) ---------- */
.anim-breathe {
  animation: breathe 1600ms ease-in-out infinite;
  transform-origin: center bottom;
}
@keyframes breathe {
  0%,100% { transform: scale(1,1); }
  50%     { transform: scale(1.02,.98); }
}

/* ---------- GLITCH SOFT (per insegne / testo) ---------- */
.glitch {
  position: relative;
  text-shadow: 1px 0 #ff3e72, -1px 0 #3ef0ff;
  animation: chroma 1500ms steps(2,end) infinite;
}
.glitch::before, .glitch::after {
  content: attr(data-text);
  position: absolute; left: 0; top: 0;
  mix-blend-mode: screen; opacity: .6; pointer-events: none;
}
.glitch::before { transform: translateX(1px); color: #ff3e72; }
.glitch::after  { transform: translateX(-1px); color: #3ef0ff; }
@keyframes chroma {
  0%, 95%  { clip-path: inset(0 0 0 0); }
  96%      { clip-path: inset(0 0 60% 0); }
  98%      { clip-path: inset(30% 0 0 0); }
  100%     { clip-path: inset(0 0 0 0); }
}

/* ---------- TREMOLO/SCOSSA (per allarmi, timer) ---------- */
.anim-shake {
  animation: shake 420ms cubic-bezier(.36,.07,.19,.97) both;
}
@keyframes shake {
  10%,90% { transform: translateX(-1px); }
  20%,80% { transform: translateX(2px); }
  30%,50%,70% { transform: translateX(-4px); }
  40%,60% { transform: translateX(4px); }
}

/* ---------- RUMORE/CRT OVERLAY (facoltativo) ---------- */
.crt {
  position: relative;
}
.crt::after{
  content:"";
  position:absolute; inset:0;
  background:
    repeating-linear-gradient(0deg, #0000 0 2px, #0001 3px 3px),
    radial-gradient(1200px 400px at 50% 120%, #0005, #0000 60%);
  opacity:.35; pointer-events:none;
  animation: scan 6s linear infinite;
}
@keyframes scan {
  0%{ background-position: 0 0, 0 0; }
  100%{ background-position: 0 6px, 0 0; }
}

/* ---------- PULSE BORDO MAPPA ---------- */
.map {
  border:3px solid #fff;
  border-radius:8px;
  box-shadow: 0 0 0 2px #2b2342, 0 0 16px #fff3;
  animation: mapPulse 1600ms ease-in-out infinite;
}
@keyframes mapPulse {
  0%,100% { box-shadow: 0 0 0 2px #2b2342, 0 0 10px #66e0ff44; }
  50%     { box-shadow: 0 0 0 2px #2b2342, 0 0 22px #66e0ffaa; }
}

/* ---------- TIMER che vira dal verde al rosso ---------- */
.timer {
  font-weight: 900;
  padding: 6px 10px;
  border: 3px solid #2b2342;
  border-radius: 8px;
  background: #0f0;
  color: #101010;
  animation: timerHue 90s linear forwards; /* 90s */
}
@keyframes timerHue {
  0%   { background:#1de042; }
  60%  { background:#ffdd33; }
  85%  { background:#ff7b2c; }
  100% { background:#ff3b3b; }
}

/* ---------- HOVER POP (bottoni direzioni) ---------- */
.btn-dir {
  display:inline-block;
  margin: 6px 8px 0 0;
  padding: 10px 16px;
  background:#ffd24a; color:#222 !important;
  border:3px solid #2b2342; border-radius:10px;
  box-shadow: 0 6px 0 #2b2342;
  transition: transform .12s ease;
}
.btn-dir:hover   { transform: translateY(-2px) scale(1.03); }
.btn-dir:active  { transform: translateY(3px) scale(.98); box-shadow: 0 2px 0 #2b2342; }

/* ---------- FADE-IN elementi secondari ---------- */
.fade-in { animation: fadeIn .6s ease both; }
@keyframes fadeIn { from { opacity:0; transform: translateY(8px);} to{opacity:1; transform:none;} }




/* === Intro Layout === */
.intro-container {
  text-align: center;
  max-width: 850px;
  margin: auto;
  padding: 20px;
  animation: fadeIn 1.5s ease-in-out;
}

.main-title {
  font-size: 60px;
  color: #ff1f8f;
  text-transform: uppercase;
  text-shadow: 3px 3px 0 #000, 0 0 20px #ff1f8f;
  margin-bottom: 25px;
  animation: titleGlow 2s infinite alternate;
}

.intro-image {
  width: 60%;
  border-radius: 12px;
  border: 2px solid #ff1a1a;
  margin-bottom: 25px;
  box-shadow: 0 0 15px rgba(255, 0, 0, 0.4);
  animation: fadeInUp 1.2s ease;
}

/* === Animations === */
@keyframes fadeIn {
  from { opacity: 0; transform: translateY(20px); }
  to { opacity: 1; transform: translateY(0); }
}

@keyframes fadeInUp {
  from { opacity: 0; transform: translateY(30px); }
  to { opacity: 1; transform: translateY(0); }
}

@keyframes titleGlow {
  0% { text-shadow: 3px 3px 0 #000, 0 0 10px #ff1f8f; }
  100% { text-shadow: 3px 3px 0 #000, 0 0 25px #ffed00; }
}
