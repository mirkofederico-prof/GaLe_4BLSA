/* TYPING AREA */
.typing {
  display: flex;
  align-items: center;
  gap: 6px;
  margin: 10px 0;
}

.hidden {
  display: none;
}

.dot {
  width: 6px;
  height: 6px;
  background: #00a2ff;
  border-radius: 50%;
  animation: typing 1s infinite;
}

.dot:nth-child(1) { animation-delay: 0s; }
.dot:nth-child(2) { animation-delay: 0.2s; }
.dot:nth-child(3) { animation-delay: 0.4s; }

.typing-text {
  font-size: 14px;
  opacity: 0.6;
}

@keyframes typing {
  0%, 20% { opacity: 0.2; }
  50% { opacity: 1; }
  80%, 100% { opacity: 0.2; }
}

/* ============================
     GLITCH ANIMATIONS
============================ */

/* titolo glitch */
.glitch-title {
  position: relative;
  display: inline-block;
  color: #ff1f8f;
  animation: glitch-skew 1.5s infinite linear alternate-reverse;
}

.glitch-title::before,
.glitch-title::after {
  content: attr(data-text);
  position: absolute;
  left: 0;
  top: 0;
  opacity: 0.7;
}

.glitch-title::before {
  color: #00eaff;
  transform: translate(2px, -2px);
  clip-path: polygon(0 0, 60% 0, 60% 100%, 0% 100%);
  animation: glitch-anim 1.5s infinite linear alternate-reverse;
}

.glitch-title::after {
  color: #ff005e;
  transform: translate(-2px, 2px);
  clip-path: polygon(40% 0, 100% 0, 100% 100%, 40% 100%);
  animation: glitch-anim2 1.4s infinite linear alternate-reverse;
}

@keyframes glitch-skew {
  0% { transform: skew(0deg); }
  20% { transform: skew(3deg); }
  40% { transform: skew(-3deg); }
  60% { transform: skew(2deg); }
  80% { transform: skew(-2deg); }
  100% { transform: skew(0deg); }
}

@keyframes glitch-anim {
  0% { clip-path: inset(0 0 90% 0); }
  20% { clip-path: inset(10% 0 60% 0); }
  40% { clip-path: inset(40% 0 30% 0); }
  60% { clip-path: inset(80% 0 0 0); }
  100% { clip-path: inset(0 0 90% 0); }
}

@keyframes glitch-anim2 {
  0% { clip-path: inset(80% 0 0 0); }
  20% { clip-path: inset(40% 0 30% 0); }
  40% { clip-path: inset(10% 0 60% 0); }
  60% { clip-path: inset(0 0 90% 0); }
  100% { clip-path: inset(80% 0 0 0); }
}

/* testo cifrato che distorce */
.glitch-text {
  animation: text-glitch 2.5s infinite;
  display: inline-block;
}

@keyframes text-glitch {
  0% { transform: translate(0,0); }
  20% { transform: translate(-1px,1px); filter: blur(1px); }
  40% { transform: translate(1px,-1px); filter: blur(0); }
  60% { transform: translate(-2px,2px); filter: blur(2px); }
  80% { transform: translate(2px,-2px); filter: blur(1px); }
  100% { transform: translate(0,0); filter: blur(0); }
}

/* flash errore */
.error-flash {
  animation: flash-error 0.25s;
}

@keyframes flash-error {
  0% { background-color: rgba(255,0,0,0.8); }
  100% { background-color: transparent; }
}

/* glitch transition block */
.glitch-transition {
  animation: glitch-transition 0.8s ease-in-out;
}

@keyframes glitch-transition {
  0% { filter: blur(0px) brightness(1); }
  30% { filter: blur(4px) brightness(2); }
  60% { filter: blur(1px) brightness(0.4); }
  100% { filter: blur(0px) brightness(1); }
}


