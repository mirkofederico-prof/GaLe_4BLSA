/* Check Answer */
setup.answerQuiz = function (choice) {
  const v = State.variables;
  const qData = setup.quizData[v.quizQ - 1];

  if (choice === qData.c) {
    v.quizQ++;
    setup.showQuizQuestion();
    return;
  }

  v.quizLives--;
  setup.updateQuizHearts();

  if (v.quizLives <= 0) {
    Engine.play("QUIZ FAIL");
    return;
