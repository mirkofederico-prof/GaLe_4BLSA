setup.checkCrossword = function () {
  const answers = {
    cw1: "simpson",
    cw2: "springfield",
    cw3: "giallo"
  };

  let correct = 0;
  let total = Object.keys(answers).length;

  Object.keys(answers).forEach(id => {
    const el = document.getElementById(id);
    if (!el) return;
    const val = el.value.trim().toLowerCase();
    if (val === answers[id]) {
      el.style.borderColor = "limegreen";
      correct++;
    } else {
      el.style.borderColor = "red";
    }
  });

  const res = document.getElementById("cw-result");
  if (!res) return;

  if (correct === total) {
    res.innerHTML = " Tutte giuste!";
    Engine.play("CrosswordOK");
  } else {
    res.innerHTML = " Alcune sono sbagliate, riprova.";
  }
};


