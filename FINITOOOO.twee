// Story JavaScript  definizioni globali per timer e cuori
setup.cifrarioTimer = null;

// usa State.variables perch  la API pi robusta
setup.startCifrarioTimer = function () {

  if (typeof State.variables.cifrarioLives === "undefined") {
    State.variables.cifrarioLives = 3;
  }
  State.variables.cifrarioTime = 120;

  setup.updateHeartsCifrario();

  if (setup.cifrarioTimer) {
    clearInterval(setup.cifrarioTimer);
    setup.cifrarioTimer = null;
  }

  setup.cifrarioTimer = setInterval(function () {

    State.variables.cifrarioTime -= 1;
    var t = State.variables.cifrarioTime;
    var timerEl = document.getElementById("cifrario-timer");

    if (timerEl) {
      timerEl.textContent = t;

      timerEl.classList.remove("timer-warning", "timer-danger");
      if (t <= 30) timerEl.classList.add("timer-danger");
      else if (t <= 60) timerEl.classList.add("timer-warning");
    }

    if (t <= 0) {
      clearInterval(setup.cifrarioTimer);
      Engine.play("CIFRARIO FAIL");
    }

  }, 1000);
};


setup.stopCifrarioTimer = function () {
  if (setup.cifrarioTimer) {
    clearInterval(setup.cifrarioTimer);
    setup.cifrarioTimer = null;
  }
};

// aggiorna i cuori del cifrario (DOM-safe)
