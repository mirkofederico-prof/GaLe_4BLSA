];

/* helper: normalizza testo */
setup._norm = function (s) {
  if (typeof s !== "string") return "";
  return s.trim().toLowerCase();
};

/* aggiorna il feedback per un singolo campo i (1..10) */
setup.updateFB = function (i) {
  var varName = "$quizQ" + i;
  var user = setup._norm(State.getVar(varName));
  var correct = setup.writtenAnswers[i-1];
  var fbEl = document.getElementById("fb" + i);
  if (!fbEl) return;
  if (user === "") {
    fbEl.textContent = ""; 
    fbEl.className = "fb";
    return;
  }
  if (user === correct) {
    fbEl.textContent = "";
    fbEl.className = "fb correct";
  } else {
    fbEl.textContent = "";
    fbEl.className = "fb wrong";
  }
};

/* aggiorna tutti i feedback */
setup.updateAllFB = function () {
  for (var i=1;i<=10;i++) setup.updateFB(i);
};

/* bind: collega eventi input -> aggiorna live
   Evitiamo doppi bind impostando setup.quizBound flag
*/
setup.bindWrittenQuiz = function () {
  if (setup.quizBound) return;
  setup.quizBound = true;

  for (let i=1;i<=10;i++) {
    (function(idx){
      var varName = "$quizQ" + idx;
      // quando l'utente scrive (macros textbox aggiorna la variabile automaticamente),
      // ascoltiamo l'evento DOMSubtreeModified sulla textbox shadow in SugarCube:
      // la soluzione pi affidabile: usare setInterval per leggere la variabile periodicamente finch si  nella pagina.
      // Ma per efficienza, possiamo usare postrender + input events: SugarCube textbox maps to input[name="twine-textbox-..."].
      // Qui useremo un polling leggero attivato per questa pagina.

      // Polling ogni 300ms per aggiornare FB per questo campo
      var last = null;
      var poll = setInterval(function(){
        // stop polling se non siamo pi nella pagina (elemento fb sparito)
        if (!document.getElementById("fb"+idx)) { clearInterval(poll); return; }
        var val = State.getVar(varName);
        if (val !== last) {
          last = val;
          setup.updateFB(idx);
        }
      }, 300);

    })(i);
  }

  // inizializza subito
  setup.updateAllFB();
};

/* conferma: se tutte corrette -> QUIZ OK, altrimenti messaggio */
setup.confirmWrittenQuiz = function () {
  var all = true;
  var wrongs = [];
  for (var i=1;i<=10;i++){
    var user = setup._norm(State.getVar("$quizQ"+i));
    var correct = setup.writtenAnswers[i-1];
    if (user !== correct) {
      all = false;
      wrongs.push(i);
    }
  }
  if (all) {
    Engine.play("QUIZ OK");
  } else {
    // evidenzia i campi sbagliati (gi visibili grazie al live check)
    // e mostra un breve messaggio toast (alert semplice)
    Engine.showError && Engine.showError("Ci sono ancora risposte sbagliate: " + wrongs.join(", "));
    // fallback alert
    if (!Engine.showError) alert("Ci sono ancora risposte sbagliate: " + wrongs.join(", "));
  }
};

