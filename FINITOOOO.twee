    const fb = document.getElementById("fb" + i);

    $(document).on("input", `[id='${id}'], input[data-var='${id}']`, function () {
      const val = setup.norm(State.variables[id]);
      const correct = setup.correctQuiz[i - 1];

      if (val === "") {
        fb.innerHTML = "";
      } else if (val === correct) {
        fb.innerHTML = "";
      } else {
        fb.innerHTML = "";
      }

      setup.updateProgressBar();
    });
  }
};

/* ##############################
   BARRA DI PROGRESSO
############################## */
setup.updateProgressBar = function () {
  let count = 0;

  for (let i = 1; i <= 10; i++) {
    const val = setup.norm(State.variables["quizQ" + i]);
    if (val === setup.correctQuiz[i - 1]) count++;
  }

  const box = document.getElementById("quiz-progress");
  if (box) {
    box.innerHTML = `${count} / 10 completate`;
  }
};

/* ##############################
   CONFERMA RISPOSTE
############################## */
setup.confirmWrittenQuiz = function () {
  let ok = true;

  for (let i = 1; i <= 10; i++) {
    const val = setup.norm(State.variables["quizQ" + i]);
    if (val !== setup.correctQuiz[i - 1]) {
      ok = false;
      break;
    }
  }

  if (ok) Engine.play("QUIZ OK");
  else Engine.play("QUIZ FAIL");
};

/* ##############################
   RESET
############################## */
setup.resetWrittenQuiz = function () {
  for (let i = 1; i <= 10; i++) {
    State.variables["quizQ" + i] = "";
    const fb = document.getElementById("fb" + i);
    if (fb) fb.innerHTML = "";
  }

  setup.updateProgressBar();
  setup.shuffleWordBank();
  setup.renderWordBank();
};


/* ------------------------
   WORD BANK BASE
------------------------*/
setup.wordBankBase = [
    "spaccio",
  "polizia",
  "ripulire",
  "estorsione",
  "protezione",
  "strada",
  "omert",
  "trafficante",
  "riciclaggio",
  "arresto"
];

/* SHUFFLE */
setup.shuffleWordBank = function () {
  setup.wordBank = [...setup.wordBankBase];
  for (let i = setup.wordBank.length - 1; i > 0; i--) {
    const j = Math.floor(Math.random() * (i + 1));
    [setup.wordBank[i], setup.wordBank[j]] = [setup.wordBank[j], setup.wordBank[i]];
  }
};

/* RENDER WORD BANK */
setup.renderWordBank = function () {
  const box = document.getElementById("wordbank-box");
  if (box) box.innerHTML = setup.wordBank.join(", ");
};

/* RISPOSTE CORRETTE */
setup.correctQuiz = [
   "spaccio",
  "polizia",
  "ripulire",
  "estorsione",
  "protezione",
  "strada",
  "omert",
  "trafficante",
  "riciclaggio",
  "arresto"
];

/* NORMALIZZA */
setup.norm = function (str) {
  return (str || "")
    .toLowerCase()
    .trim()
    .replace(/[]/g,"a")
    .replace(/[]/g,"e")
    .replace(/[]/g,"i")
    .replace(/[]/g,"o")
    .replace(/[]/g,"u");
};

/* FEEDBACK LIVE */
setup.bindWrittenQuiz = function () {
  for (let i = 1; i <= 10; i++) {
    $(document).on("input", `input[name='quizQ${i}']`, function () {
      const val = setup.norm(State.variables[`quizQ${i}`]);
      const correct = setup.correctQuiz[i - 1];
      document.getElementById(`fb${i}`).innerHTML =
        val === "" ? "" : (val === correct ? "" : "");
      setup.updateProgressBar();
    });
  }
};

/* BARRA / PROGRESS ANIMATA */
setup.updateProgressBar = function () {
  let count = 0;
  for (let i = 1; i <= 10; i++) {
    const val = setup.norm(State.variables[`quizQ${i}`]);
    if (val === setup.correctQuiz[i - 1]) count++;
  }

  const fill = document.getElementById("quiz-progress-fill");
  const text = document.getElementById("quiz-progress-text");

  if (fill) {
    fill.style.width = (count * 10) + "%";

    if (count <= 3) fill.style.background = "#ff3131";
    else if (count <= 7) fill.style.background = "#ffcc00";
    else fill.style.background = "#2ecc71";
  }

  if (text) text.innerHTML = `${count} / 10 completate`;
};

/* CONFERMA */
setup.confirmWrittenQuiz = function () {
  let ok = true;
  for (let i = 1; i <= 10; i++) {
    if (setup.norm(State.variables[`quizQ${i}`]) !== setup.correctQuiz[i - 1]) {
      ok = false;
    }
  }

  if (ok) Engine.play("QUIZ OK");
  else Engine.play("QUIZ FAIL");
};

/* RESET */
setup.resetWrittenQuiz = function () {
  for (let i = 1; i <= 10; i++) {
    State.variables[`quizQ${i}`] = "";
    const fb = document.getElementById(`fb${i}`);
    if (fb) fb.innerHTML = "";
  }
  setup.updateProgressBar();
  setup.shuffleWordBank();
  setup.renderWordBank();
};


/* =========================================================
   LABIRINTO FURTIVO  Setup & Stato
   Griglia 5x5, (0,0) in alto a sinistra
   start = (0,4)  in basso a sinistra
   exit  = (4,0)  in alto a destra
========================================================= */
setup.lab = {
  cols: 5,
  rows: 5,
  start: { x: 0, y: 4 },
  exit:  { x: 4, y: 0 },

  // celle: 0=ufficio, 1=corridoio, 2=deposito, 3=atrio (pi illuminato)
  grid: [
    [1,3,1,1,3],
    [2,1,0,1,1],
    [1,1,2,3,1],
    [0,2,1,1,2],
    [1,0,1,2,1]
  ],

  // celle illuminate (rischiose)  [x,y]
  lit: {
    "1,0":true,"4,0":true, "3,0":true,
    "3,2":true,"0,3":true, "2,4":true
  },

  // immagini per tipo stanza
  images: {
    0: "https://images.unsplash.com/photo-1524758631624-e2822e304c36?auto=format&fit=crop&w=1200&q=60", // ufficio
    1: "https://images.unsplash.com/photo-1579546929518-9e396f3cc809?auto=format&fit=crop&w=1200&q=60", // corridoio
    2: "https://images.unsplash.com/photo-1523419409543-9e79b3c7b1e1?auto=format&fit=crop&w=1200&q=60", // deposito
    3: "https://images.unsplash.com/photo-1516637090014-cb1ab0d08fc7?auto=format&fit=crop&w=1200&q=60"  // atrio/ingresso
  },

  titles: {
    0: "Ufficio laterale",
    1: "Corridoio",
    2: "Deposito",
    3: "Atrio illuminato"
  },

  descs: {
    0: "Scrivanie, schedari, fumo di sigaretta. Ombre lunghe.",
    1: "Pareti spoglie, passi che rimbombano. Meglio muoversi piano.",
    2: "Scatoloni e odore di polvere. Buon posto per nascondersi.",
    3: "Lampade accese e videocamere. Ogni rumore pu tradirti."
  }
};

setup.labInit = function () {
  const v = State.variables;
  const L = setup.lab;

  v.labX = L.start.x;
  v.labY = L.start.y;
  v.labAlerts = 0;
  v.labTime = 90;   // secondi
  v.labExplored = {}; // "x,y" -> true
  v.labTimerRunning = false;
};

setup._coordKey = function (x, y) {
  return x + "," + y;
};

setup._inBounds = function (x, y) {
  const L = setup.lab;
  return x >= 0 && y >= 0 && x < L.cols && y < L.rows;
};

setup._cellType = function (x, y) {
  return setup.lab.grid[y][x];
};

setup._isLit = function (x, y) {
  return !!setup.lab.lit[ setup._coordKey(x,y) ];
};

/* ---------------------------------------------------------
   Timer 90s
--------------------------------------------------------- */
setup._labTimerId = null;

setup.labStartTimer = function () {
  const v = State.variables;
  if (v.labTimerRunning) return;
  v.labTimerRunning = true;

  // render una volta
  setup._renderTimer();

  setup._labTimerId = setInterval(function () {
    v.labTime -= 1;
    if (v.labTime <= 0) {
      v.labTime = 0;
      setup._renderTimer();
      setup.labGameOver("time");
      return;
    }
    setup._renderTimer();
  }, 1000);
};

setup.labStopTimer = function () {
  const v = State.variables;
  v.labTimerRunning = false;
  if (setup._labTimerId) {
    clearInterval(setup._labTimerId);
    setup._labTimerId = null;
  }
};

setup._renderTimer = function () {
  const v = State.variables;
  const el = document.getElementById("lab-timer");
  if (!el) return;
  // animazione colore a soglia
  if (v.labTime <= 15) {
    el.classList.add("danger");
  } else if (v.labTime <= 40) {
    el.classList.add("warn");
  }
  el.textContent = " " + v.labTime + "s";
};

/* ---------------------------------------------------------
   Render UI (stanza, mappa, bottoni)
--------------------------------------------------------- */
setup.labRender = function () {
  const v = State.variables;
  const L = setup.lab;

  // marca esplorata
  v.labExplored[ setup._coordKey(v.labX, v.labY) ] = true;

  // titolo + descrizione + badge luce
  const t = setup._cellType(v.labX, v.labY);
  const titleEl = document.getElementById("lab-room-title");
  const descEl  = document.getElementById("lab-room-desc");
  const imgEl   = document.getElementById("lab-room-image");
  const badgeEl = document.getElementById("lab-room-badge");
  const alertNum = document.getElementById("lab-alert-num");

  if (titleEl) { 
    titleEl.setAttribute("data-text", setup.lab.titles[t]);
    titleEl.innerText = setup.lab.titles[t]; 
  }
  if (descEl)  { descEl.innerText  = setup.lab.descs[t]; }
  if (imgEl)   { imgEl.src         = setup.lab.images[t]; }

  if (badgeEl) {
    if (setup._isLit(v.labX, v.labY)) {
      badgeEl.innerHTML = "ILLUMINATO";
      badgeEl.classList.add("lit");
    } else {
      badgeEl.innerHTML = "IN OMBRA";
      badgeEl.classList.remove("lit");
    }
  }

  if (alertNum) { alertNum.textContent = v.labAlerts; }

  // mappa
  setup._renderMap();

  // bottoni movimento
  const canN = setup._inBounds(v.labX, v.labY-1);
  const canS = setup._inBounds(v.labX, v.labY+1);
  const canE = setup._inBounds(v.labX+1, v.labY);
  const canW = setup._inBounds(v.labX-1, v.labY);

  setup._bindBtn("btnN", canN, function(){ setup.labMove(0,-1); });
  setup._bindBtn("btnS", canS, function(){ setup.labMove(0, 1); });
  setup._bindBtn("btnE", canE, function(){ setup.labMove(1, 0); });
  setup._bindBtn("btnW", canW, function(){ setup.labMove(-1,0); });

  setup._bindBtn("btnHide", true, function(){
    // nasconditi: riduci allarmi di 1 (min 0), consuma 1s bonus
    v.labAlerts = Math.max(0, v.labAlerts - 1);
    setup._renderTimer(); // gi si aggiorna ogni secondo, ma ripassiamo
    setup.labRender();
    setup._flashMsg("Ti acquatti nellombra. Rischio ridotto di 1.", "ok");
  });

  // se siamo alluscita, vittoria
  if (v.labX === L.exit.x && v.labY === L.exit.y) {
    setup.labStopTimer();
    Engine.play("LAB WIN");
  }
};

setup._bindBtn = function (id, enabled, handler) {
  const b = document.getElementById(id);
  if (!b) return;
  b.disabled = !enabled;
  b.onclick = enabled ? handler : null;
};

setup._renderMap = function () {
  const v = State.variables;
  const L = setup.lab;
  const map = document.getElementById("lab-map");
  if (!map) return;

  let html = "";
  for (let y=0; y<L.rows; y++) {
    html += `<div class="lab-map-row">`;
    for (let x=0; x<L.cols; x++) {
      const key = setup._coordKey(x,y);
      const explored = !!v.labExplored[key];
      const isCurrent = (x===v.labX && y===v.labY);
      const isExit = (x===L.exit.x && y===L.exit.y);

      let cls = "cell";
      if (explored) cls += " explored";
      if (isCurrent) cls += " current";
      if (isExit) cls += " exit";

      html += `<div class="${cls}"></div>`;
    }
    html += `</div>`;
  }
  map.innerHTML = html;
};

/* ---------------------------------------------------------
   Movimento + Stealth
--------------------------------------------------------- */
setup.labMove = function (dx, dy) {
  const v = State.variables;

  const nx = v.labX + dx;
  const ny = v.labY + dy;
  if (!setup._inBounds(nx, ny)) return;

  // spostati
  v.labX = nx;
  v.labY = ny;

  // rischio: se cella illuminata, 1 chance su 2 di alzare allarme
  if (setup._isLit(nx, ny)) {
    if (Math.random() < 0.5) {
      v.labAlerts += 1;
      setup._flashMsg("Telecamera! Lallarme sale.", "warn");
    } else {
      setup._flashMsg("Passi veloci sotto le luci per ora tutto ok.", "ok");
    }
  } else {
    setup._flashMsg("Ti muovi tra le ombre.", "ok");
  }

  // check allarmi
  if (v.labAlerts >= 3) {
    setup.labGameOver("alerts");
    return;
  }

  setup.labRender();
};

setup.labGameOver = function (why) {
  setup.labStopTimer();
  Engine.play("LAB FAIL");
};

/* ---------------------------------------------------------
   Messaggi flash in basso
--------------------------------------------------------- */
setup._flashMsg = function (text, type) {
  const el = document.getElementById("lab-msg");
  if (!el) return;
  el.innerText = text;
  el.classList.remove("ok","warn");
  if (type) el.classList.add(type);
  el.classList.add("show");
  setTimeout(() => el.classList.remove("show"), 1300);
};



